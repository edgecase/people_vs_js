!= partial('header')

ul.participants

#mainPanelSwitch
  #questionsSwitch Questions
  #slidesSwitch.active Slides

#panel_container
  #left_panel.panel
    .msg
    #slidesContainer
      iframe(src="http://saterus.github.com/haskell_talk/#(27)", frameborder="0", border="0", cellspacing="0")
    .questionContainer
      #welcome
        h1 Welcome and Begin!
        input(type="hidden", name="name", id="name", value="The_Presenter")
        br
        input(type="button", id="name_button", value="Login as Presenter", class="enabled")

  #right_panel.panel
    #answer_pad.section
      ul.possibleAnswers
      ul.answerStats

      #final_answer_container
        a#final_answer.disabled(href="#") Submit My Final Answer

      #credits.presenter
        a(href="#", id="presenterBack") back
        &nbsp;
        a(href="#", id="presenterNext") next
        &nbsp;
        a(href="#", id="presenterResetQuiz") reset

    #discussion_pad.section
      #discussion_enter
        #textarea_container
          textarea.discussion
        input(type="button", id="submit_discussion", value="Send", class="enabled")
      #discussion_area
        table#discussion_items
